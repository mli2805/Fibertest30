IIT-ALARMS-MIB DEFINITIONS ::= BEGIN

IMPORTS	enterprises
		FROM RFC1155-SMI
	OBJECT-TYPE
		FROM RFC1212
	TRAP-TYPE
		FROM RFC-1215;

--
-- Node definitions
--

-- 1.3.6.1.4.1.36220
iit OBJECT IDENTIFIER ::= { enterprises 36220 }

-- 1.3.6.1.4.1.36220.30
iit-ft-30 OBJECT IDENTIFIER ::= { iit 30 }

-- 1.3.6.1.4.1.36220.30.0
event_id OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS mandatory
	::= { iit-ft-30 0 }

-- 1.3.6.1.4.1.36220.30.1
event_registration_time OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS mandatory
	::= { iit-ft-30 1 }

-- 1.3.6.1.4.1.36220.30.2
rtu_title OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS mandatory
	::= { iit-ft-30 2 }

-- 1.3.6.1.4.1.36220.30.3
trace_title OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS optional
	::= { iit-ft-30 3 }

-- 1.3.6.1.4.1.36220.30.10
rtu_main_channel OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS optional
	::= { iit-ft-30 10 }

-- 1.3.6.1.4.1.36220.30.11
rtu_reserve_channel OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS optional
	::= { iit-ft-30 11 }


-- 1.3.6.1.4.1.36220.30.20
bop_title OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS optional
	::= { iit-ft-30 20 }
	
-- 1.3.6.1.4.1.36220.30.21
bop_state OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS optional
	::= { iit-ft-30 21 }
	
-- 1.3.6.1.4.1.36220.30.30
trace_state OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS optional
	::= { iit-ft-30 30 }
	
-- 1.3.6.1.4.1.36220.30.31
