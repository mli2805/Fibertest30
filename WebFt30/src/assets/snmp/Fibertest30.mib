	STATUS optional
	::= { iit-ft-30 702 }
	

--
-- Trap definitions, Return
--

optical_event TRAP-TYPE
	ENTERPRISE iit-ft-30
	VARIABLES { event_id, event_registration_time, rtu_title, trace_title, trace_state, 
		accident_node_title, accident_type, accident_distance, accident_gps, 
		left_node_title, left_node_gps, left_node_distance_to_rtu,
		right_node_title, right_node_gps, right_node_distance_to_rtu}
	::= 100

rtu_network_event TRAP-TYPE
	ENTERPRISE iit-ft-30
	VARIABLES { event_id, event_registration_time, rtu_title, rtu_main_channel, rtu_reserve_channel }
	::= 200

bop_network_event TRAP-TYPE
	ENTERPRISE iit-ft-30
	VARIABLES { event_id, event_registration_time, rtu_title, bop_title, bop_state }
	::= 300

rtu_status_event TRAP-TYPE
	ENTERPRISE iit-ft-30
	VARIABLES { event_id, event_registration_time, rtu_title, trace_title, base_ref_type, rtu_status_event_type, rtu_status_event_name }
	::= 400

test_trap TRAP-TYPE
	ENTERPRISE iit-ft-30
	VARIABLES { test_string, event_registration_time, interger_number, floating_point_number }
	::= 777


END