-- 1.3.6.1.4.1.36220.30.42
left_node_distance_to_rtu OBJECT-TYPE
	SYNTAX OCTET STRING
	ACCESS read-only
	STATUS optional
	::= { iit-ft-30 42 }
	
-- 1.3.6.1.4.1.36220.30.50
right_node_title OBJECT-TYPE
	SYNTAX OCTET STRING
